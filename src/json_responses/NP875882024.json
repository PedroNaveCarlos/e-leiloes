        "titulo": "im\u00f3vel com 8000 m2 destinado a armaz\u00e9ns e ind\u00fastria, com logradouro, em Sintra",
        "valorBase": 2013660.38,
        "valorAbertura": 1006830.19,
        "valorMinimo": 1711611.32,
        "tipo": "Im\u00f3vel",
